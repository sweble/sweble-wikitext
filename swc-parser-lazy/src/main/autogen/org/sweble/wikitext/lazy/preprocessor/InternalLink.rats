module org.sweble.wikitext.lazy.preprocessor.InternalLink;

modify org.sweble.wikitext.lazy.preprocessor.Content;




// -- InternalLink -------------------------------------------------------------

AstNode InternalLink =
    "[[" content:InternalLinkContentStar "]]"
    {
      yyValue = new NodeList(new Text("[["), content, new Text("]]"));
    }
  / '['
    {
      yyValue = new Text("[");
    }
;




// -- InternalLink Content -----------------------------------------------------

void ContentStringStopper +=
    <PrefixOnlyInclude>    ...
  / <PrefixClosingBraces>  "]]"
;

NodeList InternalLinkContentStar = ContentStar ;




// -- End of file -------------------------------------------------------------

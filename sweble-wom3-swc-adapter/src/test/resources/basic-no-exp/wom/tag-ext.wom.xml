<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<article xmlns="http://sweble.org/schema/wom30"
         xmlns:mww="http://sweble.org/schema/mww30"
         version="3.0"
         title="Tag-ext">
   <body>
      <p topgap="0" bottomgap="0">
         <mww:tagext name="pre">
            <rtd>&lt;pre</rtd>
            <mww:attr>
               <rtd> </rtd>
               <mww:name>
                  <text>attr</text>
               </mww:name>
               <rtd>="</rtd>
               <mww:value>
                  <text>value</text>
               </mww:value>
               <rtd>"</rtd>
            </mww:attr>
            <mww:attr>
               <rtd> </rtd>
               <mww:name>
                  <text>width</text>
               </mww:name>
               <rtd>="</rtd>
               <mww:value>
                  <text>5</text>
               </mww:value>
               <rtd>"</rtd>
            </mww:attr>
            <rtd>&gt;</rtd>
            <mww:body>
               <text>I'm a tag extension</text>
            </mww:body>
            <rtd>&lt;/pre&gt;</rtd>
         </mww:tagext>
      </p>
   </body>
</article>

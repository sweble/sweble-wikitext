<project xmlns="http://maven.apache.org/DECORATION/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd"
    name="Sweble Wiki Object Model Version 3" >

  <bannerLeft>
    <name>Sweble</name>
    <src>http://sweble.org/img/maven-site-banner-left-sweble-wom3.png</src>
    <href>http://sweble.org</href>
  </bannerLeft>
  <bannerRight>
    <src>http://sweble.org/img/maven-site-banner-right.png</src>
  </bannerRight>

  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.1</version>
  </skin>

  <version position="right" />
  <publishDate position="right" />

  <custom>
    <fluidoSkin>
      <topBarEnabled>false</topBarEnabled>
      <sideBarEnabled>true</sideBarEnabled>
    </fluidoSkin>
  </custom>

  <body>

    <head />

    <breadcrumbs>
      <item name="Sweble" href="http://sweble.org" />
      <item name="Documentation" href="http://sweble.org/doc" />
      <item name="Tooling" href="${project.url}/../.." />
      <item name="Sweble" href="${project.url}/.." />
    </breadcrumbs>

    <menu ref="parent"/>
    <menu ref="modules"/>

    <menu ref="reports"/>

    <menu name="Sweble">
      <item name="Blog" href="http://sweble.org" />
      <item name="Documentation" href="http://sweble.org/doc" />
      <item name="Mailing Lists" href="http://sweble.org/mailing-lists" />
      <item name="Repositories" href="http://sweble.org/repos" />
      <item name="Bugtracker" href="http://sweble.org/bugtracker" />
    </menu>

  </body>
</project>
